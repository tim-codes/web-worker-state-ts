#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/bin/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/rollup@2.62.0/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules:/Users/tim/dev/personal/node_modules:/Users/tim/dev/node_modules:/Users/tim/node_modules:/Users/node_modules:/node_modules:/Users/tim/dev/personal/map-worker-ts/packages/worker/node_modules:/Users/tim/dev/personal/map-worker-ts/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/bin/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/rollup@2.62.0/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules/.pnpm/node_modules:/Users/tim/dev/personal/map-worker-ts/node_modules:/Users/tim/dev/personal/node_modules:/Users/tim/dev/node_modules:/Users/tim/node_modules:/Users/node_modules:/node_modules:/Users/tim/dev/personal/map-worker-ts/packages/worker/node_modules:/Users/tim/dev/personal/map-worker-ts/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/bin/rollup" "$@"
fi
